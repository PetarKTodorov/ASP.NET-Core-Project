@model IEnumerable<Song>

<div class="row">
    @foreach (var song in Model)
    {
        if (song.IsDeleted == false)
        {
            <div class="col-6 border border-dark">
                <div> Is Deleted: @song.IsDeleted</div>
                <div>@song.Id</div>
                <div>@song.Name</div>
                <div>@song.RelaseDate</div>
                <div>
                    <a asp-area="Admin" asp-controller="Song" asp-action="Edit" asp-route-id="@song.Id">Edit </a>
                    <a asp-area="Admin" asp-controller="Song" asp-action="Delete" asp-route-id="@song.Id">Delete </a>
                </div>
            </div>
        }

    }
</div>
