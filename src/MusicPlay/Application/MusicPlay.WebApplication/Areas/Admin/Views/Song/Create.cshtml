@model Song;

<form asp-area="Admin" asp-controller="Song" asp-action="Create" method="POST">

    <div class="form-group">
        <label asp-for="@Model.Name"></label>
        <input class="form-control" asp-for="@Model.Name" />
    </div>

    <div class="form-group">
        <label asp-for="@Model.RelaseDate"></label>
        <input class="form-control" asp-for="@Model.RelaseDate" />
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>